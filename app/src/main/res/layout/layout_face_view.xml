<?xml version="1.0" encoding="utf-8"?>
<merge xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <com.dafay.demo.lib.base.ui.widgets.SquareFrameLayout
        android:id="@+id/fl_face_container"
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_face_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_left_eye_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_left_cheek_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_right_eye_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_right_cheek_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />


        <com.dafay.demo.robot.ui.view.CurveShapeView
            android:id="@+id/cv_mouse_orbit"
            android:layout_width="match_parent"
            android:layout_height="match_parent" />

    </com.dafay.demo.lib.base.ui.widgets.SquareFrameLayout>


</merge>